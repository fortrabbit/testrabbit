{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "~7.1",
        "ext-apcu": "*",
        "ext-imagick": "*",
        "fideloper/proxy": "^4.0",
        "fzaninotto/faker": "^1.8",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "mongodb/mongodb": "^1.4",
        "rcrowe/twigbridge": "^0.9.8"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-install-cmd": [
            "php artisan migrate --force"
        ]
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": {
            "fortrabbit/*": "source",
            "frbit/*": "source",
            "*": "dist"
        },
        "platform": {
            "php": "7.3"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
